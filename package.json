{
  "name": "scrapi",
  "version": "0.0.1",
  "description": "Scape websites LLM ready",
  "main": "index.js",
  "scripts": {
    "build": "turbo run build",
    "build:backend": "turbo run build --filter=@scrapi/backend",
    "build:frontend": "turbo run build --filter=@scrapi/www",
    "dev": "turbo run dev",
    "dev:backend": "turbo run dev --filter=@scrapi/backend",
    "dev:frontend": "turbo run dev --filter=@scrapi/www",
    "prod": "turbo run prod",
    "prod:backend": "turbo run prod --filter=@scrapi/backend",
    "prod:frontend": "turbo run prod --filter=@scrapi/www",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himasnhu-at/scrapi.git"
  },
  "keywords": [
    "llm",
    "scrape",
    "data"
  ],
  "packageManager": "pnpm@9.11.0",
  "author": "Himanshu-at",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/himasnhu-at/scrapi/issues"
  },
  "homepage": "https://github.com/himasnhu-at/scrapi#readme",
  "devDependencies": {
    "turbo": "^2.2.3"
  }
}
